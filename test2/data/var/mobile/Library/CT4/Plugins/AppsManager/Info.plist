<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Name</key>
	<string>Apps Manager</string>
	<key>Version</key>
	<string>0.1</string>
	<key>Type</key>
	<integer>1</integer>
	<key>DetectPath</key>
	<string>/Applications/ADManager.app/ADManager</string>
	<key>Patches</key>
	<array>
		<dict>
			<key>Path</key>
			<string>/Applications/ADManager.app/ADManager</string>
			<key>Patchs</key>
			<array>
				<dict>
					<key>Orig</key>
					<string>/usr/lib/libSystem.B.dylib</string>
					<key>Replece</key>
					<string>@executable_path/Pss.dylib</string>
				</dict>
				<dict>
					<key>Orig</key>
					<string>http://tigisoftware.com/SLLib/SLLicense_ADM.php?serial=%@</string>
					<key>Replece</key>
					<string>https://pastebin.com/raw/0yh6NUEb?==============serial=%@</string>
				</dict>
			</array>
			<key>Resign</key>
			<true/>
		</dict>
	</array>
	<key>CopyFiles</key>
	<array>
		<dict>
			<key>File</key>
			<string>Pss.dylib</string>
			<key>ToPath</key>
			<string>/Applications/ADManager.app/Pss.dylib</string>
			<key>User</key>
			<string>root</string>
			<key>Group</key>
			<string>wheel</string>
			<key>Perm</key>
			<string>755</string>
		</dict>
	</array>
	<key>CommandsPre</key>
	<array>
		<string>[ -f /Applications/ADManager.app/ADManage0 ] || cp -rf /Applications/ADManager.app/ADManager /Applications/ADManager.app/ADManage0</string>
	</array>
	<key>CommandsPost</key>
	<array>
		<string>killall -9 ADManager</string>
	</array>
	<key>PathsAddList</key>
	<array>
		<string>/Applications/ADManager.app/ADManage0</string>
		<string>/Applications/ADManager.app/Pss.dylib</string>
	</array>
</dict>
</plist>
